  month = sep,
  year = {2016},
  pages = {651--668},
  file = {Full Text PDF:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/XD77JTXE/Gathiaka et al. - 2016 - D3R grand challenge 2015 Evaluation of protein–li.pdf:application/pdf;Snapshot:/Users/dmobley/Library/Application Support/Zotero/Profiles/i2jd8b87.default/zotero/storage/I6GRFPMA/s10822-016-9946-8.html:text/html}
